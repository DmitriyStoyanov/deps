            "versions": "Y.Y.Y"
          },
          {
            "name": "django-.*",
            "enabled": true,
            "versions": "Y.Y.Y"
          },
          {
            "name": "django",
            "enabled": true,
            "versions": "L.Y.Y"
          },
          {
            "name": ".*",
            "enabled": true,
            "versions": "Y.Y.Y"
          }
        ]
      }
    }
